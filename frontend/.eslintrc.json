{
  "extends": [
    "eslint:recommended",
    "next/core-web-vitals",
    "prettier"
  ],
  "rules": {
    "no-unused-vars": [
      "warn",
      {
        "varsIgnorePattern": "^_"
      }
    ]
  },
  "overrides": [
    {
      "files": [
        "**/*.test.js"
      ],
      "rules": {
        "no-undef": "off",
        "no-global-assign": "off"
      }
    }
  ]
}